algoritmo "Creuza01"
// Função :
// Autor :
// Data : 10/08/2023
// Seção de Declarações 
var
    ano_atual, ano_nasc, idade: inteiro
inicio
      Escreval("Em que ano nós estamos? ")
      leia(ano_atual)
      escreval("Em que ano eu nasci? ")
      leia(ano_nasc)
      idade <- ano_atual - ano_nasc
      escreva("Minha idade sera ", idade, " anos")
fimalgoritmo
