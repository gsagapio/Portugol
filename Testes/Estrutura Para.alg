algoritmo "Estrutura_Para"
// Função :
// Autor :
// Data : 09/09/2023
// Seção de Declarações 
var
  c: inteiro
inicio
// Seção de Comandos 
   Para c <- 0 ate 20 passo 2 faca
      escreval(c)
   fimpara
   
   Para c <- 10 ate 1 passo -1 faca
      escreval(c)
   fimpara
   
   Para c <- 1 ate 5 faca
      escreval(c)
   fimpara
fimalgoritmo
