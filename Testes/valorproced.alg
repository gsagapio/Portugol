algoritmo "Fazsomaprocedimento"
// Função :
// Autor :
// Data : 09/10/2023
// Seção de Declarações 
var
   x, y: inteiro
   
procedimento soma(a, b: inteiro)
inicio
      escreval("Recebi o valor ", a)
      escreval("Recebi o valor ", b)
      escreval("A soma entre os dois é igual a ", a + b)
fimprocedimento
inicio
// Seção de Comandos
   x <- 5
   y <- 3
   
   soma(x, y)
fimalgoritmo
