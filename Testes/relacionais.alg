algoritmo "valores_relacionais"
// Função :
// Autor :
// Data : 03/08/2023
// Seção de Declarações 
var
   n1, n2, n3: inteiro
   
inicio
      n1 <- 2
      n2 <- 3
      n3 <- 5
      escreval(n1>n2)
      escreval(n1=n2)
      escreval(n1<n2)
      escreval(n1>=n2)
      escreval(n1<=n2)
      escreval(n1<>n2)
      escreva (n3 = n2 + n1)
fimalgoritmo
